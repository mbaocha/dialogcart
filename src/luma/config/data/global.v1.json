    "entity_types": {
        "date": {
            "token": "datetoken",
            "weekday": {
                "to_number": {
                    "monday": 0,
                    "tuesday": 1,
                    "wednesday": 2,
                    "thursday": 3,
                    "friday": 4,
                    "saturday": 5,
                    "sunday": 6
                }
            },
            "month": {
                "to_number": {
                    "january": 1,
                    "february": 2,
                    "march": 3,
                    "april": 4,
                    "may": 5,
                    "june": 6,
                    "july": 7,
                    "august": 8,
                    "september": 9,
                    "october": 10,
                    "november": 11,
                    "december": 12
                }
            },
            "relative": [
                {
                    "value": "today",
                    "offset_days": 0
                },
                {
                    "value": "tomorrow",
                    "offset_days": 1
                },
                {
                    "value": "tonight",
                    "offset_days": 0
                },
                {
                    "value": "next week",
                    "offset_days": 7
                },
                {
                    "value": "next month",
                    "offset_days": 30
                }
            ],
            "absolute": {
                "token": "datetoken",
                "patterns": [
                    {
                        "id": "day_month_text",
                        "type": "regex",
                        "value": "\\b(\\d{1,2})(?:st|nd|rd|th)?\\s+(jan|january|feb|february|mar|march|apr|april|may|jun|june|jul|july|aug|august|sep|september|oct|october|nov|november|dec|december)(?:\\s+(\\d{4}))?\\b"
                    },
                    {
                        "id": "month_day_text",
                        "type": "regex",
                        "value": "\\b(jan|january|feb|february|mar|march|apr|april|may|jun|june|jul|july|aug|august|sep|september|oct|october|nov|november|dec|december)\\s+(\\d{1,2})(?:st|nd|rd|th)?(?:\\s+(\\d{4}))?\\b"
                    },
                    {
                        "id": "numeric_date",
                        "type": "regex",
                        "value": "\\b(\\d{1,2})[/-](\\d{1,2})(?:[/-](\\d{2,4}))?\\b"
                    }
                ]
            }
        },
        "time": {
            "token": "timetoken",
            "patterns": [
                {
                    "id": "time_12h",
                    "type": "regex",
                    "value": "\\b(1[0-2]|0?[1-9])([:.][0-5][0-9])?\\s?(am|pm)\\b",
                    "synonym": [
                        "12pm",
                        "12 pm",
                        "12.30pm",
                        "9am"
                    ]
                },
                {
                    "id": "time_24h",
                    "type": "regex",
                    "value": "\\b([01]?[0-9]|2[0-3]):[0-5][0-9]\\b",
                    "synonym": [
                        "15:00",
                        "09:30"
                    ]
                }
            ],
            "keywords": [
                {
                    "value": "morning",
                    "granularity": "window",
                    "range": [
                        "08:00",
                        "11:59"
                    ]
                },
                {
                    "value": "afternoon",
                    "granularity": "window",
                    "range": [
                        "12:00",
                        "16:59"
                    ]
                },
                {
                    "value": "evening",
                    "granularity": "window",
                    "range": [
                        "17:00",
                        "20:59"
                    ]
                },
                {
                    "value": "night",
                    "granularity": "window",
                    "range": [
                        "21:00",
                        "23:59"
                    ]
                }
            ],
            "timewindowtoken": "timewindowtoken"
        },
        "duration": {
            "token": "durationtoken",
            "patterns": [
                {
                    "type": "regex",
                    "value": "\\b(\\d+)\\s?(minutes|minute|mins|min)\\b",
                    "unit": "minutes"
                },
                {
                    "type": "regex",
                    "value": "\\b(\\d+)\\s?(hours|hour|hrs|hr)\\b",
                    "unit": "hours"
                }
            ],
            "keywords": [
                {
                    "value": "half hour",
                    "minutes": 30
                },
                {
                    "value": "one hour",
                    "minutes": 60
                }
            ]
        }
